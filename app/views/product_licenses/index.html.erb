<div id="outer_block" class="alert " style="display: none"></div><br />
<div class="inline reports">
	<h1>Product Licenses</h1>
	</div>

<div class="container-div table-responsive" id="licences_div">

	
<% unless @product_licenses.nil?%>

<table  class="table  table-striped table-bordered" id="license_table">
 <thead>
  <tr>
  	<th>License key</th>
    <th>Calculated key</th>
    <th>Email11</th>
    <th>Machine id</th>
    <th>Voices</th>
    <!-- <th>Sangeeta</th>
    <th>Sulekha</th> -->
    <!-- <th>Expiry date</th> -->
    <!-- <th>Reset counter</th>
    <th>Allow regeneration</th> -->
    <th>Is assigned</th>
    <th>Is Key Generated</th>
    <!-- <th>Is deleted</th> -->
    <!-- <th>Created by</th>
    <th>Updated by</th>
    <th>Updated date</th>
    -->
    <th></th>
  </tr>
</thead>
<tbody>
<% @product_licenses.each do |product_license| %>
  <tr>
  	<td><%= product_license.license_key %></td>
    <td><%= product_license.calculated_key %></td>
    <td><%= product_license.email %></td>
    <td><%= product_license.machine_id %></td>
    <%all_products = ""%>
    <% product_license.products.each do |product| %>
    	<% all_products= product.name.to_s+","+all_products%>
    <%end %>
    <td><%=all_products%></td>
    <!-- <td><%= product_license.expiry_date %></td> -->
    <!-- <td><%= product_license.reset_counter %></td>
    <td><%= product_license.allow_regeneration %></td> -->
    <%if product_license.is_assigned?%>
    <td>Yes</td>
    <%else%>
    <td>No</td>
    <%end%>
    <%if product_license.is_created?%>
    <td>Yes</td>
    <%else%>
    <td>No</td>
    <%end%>
    <!-- <td><%= product_license.is_deleted %></td> -->
    <!-- <td><%= product_license.created_by %></td>
    <td><%= product_license.updated_by %></td>
    <td><%= product_license.updated_date %></td>    -->
    <td><input type="checkbox" class="select_item" value="<%= product_license.id%>"/></td> 
  </tr>
<% end %>
</tbody>
</table>
<%end%>

<script>
	$(document).ready(function(){
		$("#license_table").dataTable({
			"sPaginationType": "bootstrap"
		});	
	});
</script>
 
<br />
<br />
<a id="reset_key" href="#" class="btn btn-large btn-primary">Reset Key</a>
<a id="delete_key" href="#" class="btn btn-large btn-primary">Delete Key</a>
</div>


<div id="response_div"></div>


