

<% unless @searched_emails.nil?%>
	<table class="table table-striped table-bordered table-condensed">
		<tr>
  	<th>Product Name</th>
    <th>License key</th>
    <th>Calculated key</th>
    <th>Email</th>
    <th>Machine id</th>
    <th>Expiry date</th>
    <!-- <th>Reset counter</th>-->
    <th>reset key</th>
    <th>Is assigned</th>
    <th>Is created</th>
    <th>Is deleted</th>
    <!-- <th>Created by</th>
    <th>Updated by</th>-->
    <th>Action</th>
    
  </tr>
	
	<% searched_email = @searched_emails%>
  <tr>
  	<%all_products = ""%>
    <% searched_email.products.each do |product| %>
    	<% all_products= product.name.to_s+","+all_products%>
    <%end %>
  	<td><%= all_products%></td>
    <td><%= searched_email.license_key %></td>
    <td><%= searched_email.calculated_key %></td>
    <td><%= searched_email.email %></td>
    <td><%= searched_email.machine_id %></td>
    <td><%= searched_email.expiry_date %></td>
    <!-- <td><%= searched_email.reset_counter %></td>-->
    <% if searched_email.allow_regeneration%>
    <td>Yes</td>
    <%else%>
    <td>No</td>
    <%end%>
    <% if searched_email.is_assigned%>
    <td>Yes</td>
    <%else%>
    <td>No</td>
    <%end%>
    <% if searched_email.is_created%>
    <td>Yes</td>
    <%else%>
    <td>No</td>
    <%end%>
	<% if searched_email.is_deleted %>
    <td>Yes</td>
    <%else%>
    <td>No</td>
    <%end%>
    <!-- <td><%= searched_email.created_by %></td>
    <td><%= searched_email.updated_by %></td>  --> 
    <%if searched_email.allow_regeneration%>
    <td >allow</td>
    <%else%>
    <td ><a style="color: blue" href="#" id="re_gen_id" license_id="<%= searched_email.id %>">Allow</a></td>
    <%end%>  
  </tr>
  </table>

<%else%>
	<div id="outer_block" class="alert" >
		<p> Sorry! No keys found for this email</p>
			
	</div><br />
<%end%>
