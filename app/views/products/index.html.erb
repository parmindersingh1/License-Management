<div id="generated_license">
<h2>All products</h2>
<div id="outer_block" class="alert" style="display: none"></div><br />
<table class="table table-striped table-bordered table-condensed">
  <tr>
    <th></th>
    <th>Name</th>
    <th>Version</th>
    <th>Release date</th>
    

  </tr>

<% @products.each do |product| %>
  <tr>
    <td><input type="checkbox" class="product_name" value="<%= product.id%>" checked/></td>
    <td><%= product.name %></td>
    <td><%= product.version %></td>
    <td><%= product.release_date %></td>

    
  </tr>
<% end %>
</table>

<br />
<input type="text" class="input-block-level" placeholder="enter no of keys to generate" id="keys_count">
<br />
<br />
<a id="new_product" href="/products/new" class="btn btn-large btn-primary">Add New Product</a>
<a id="delete_product" href="#" class="btn btn-large btn-primary">Delete Product</a>
<input type="button" id="generate_button" class="btn btn-large btn-primary" value="Generate Licenses" />

<%=render :partial => "product_model_box"%>
</div>


