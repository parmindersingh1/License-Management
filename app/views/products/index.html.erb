<h2>All products</h2>
<div id="outer_block" class="alert " style="display: none"></div><br />
<table class="table table-striped table-bordered table-condensed">
  <tr>
    <th>Name</th>
    <th>Version</th>
    <th>Release date</th>
    
  </tr>

<% @products.each do |product| %>
  <tr>
    <td><%= product.name %></td>
    <td><%= product.version %></td>
    <td><%= product.release_date %></td>
    
	

  </tr>
<% end %>
</table>

<br />

<a id="new_product" href="/products/new" class="btn btn-large btn-primary">Add New Product</a>
<input type="button" id="licenses_gen" class="btn btn-large btn-primary" value="Genrate Licenses" />
<%=render :partial => "product_model_box"%>


